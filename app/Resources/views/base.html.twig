<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta charset="UTF-8" />
        <title>{% block title %}baseTitle{% endblock %} | PFC</title>
        {% block stylesheets %}{% endblock %}
        {% block rss %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('PFC_icons/1_Desktop_Icons/icon_016.png') }}" />
    </head>
    <body>
        <div id="contenedor">
            {% block body %}{% endblock %}
            <footer>
                &copy; {{ 'now'|date('Y') }}
                <a href="{{ path('proves')                                      }}">proves                  </a>
            </footer>           
            {% block javascripts %}{% endblock %}
        </div>
        <div style="float:left;border:2px LightCoral dashed;background-color:linen;margin-bottom:3em;">
            <h3>MAPA</h3>
            <ul style="float:left;width:10em;border:1px green solid;">
                <li><a href="{{ path('portada')                                     }}">Portada                 </a></li>
                <li><a href="{{ path('pagina_estatica',{'pagina':'sobre'})          }}">Sobre Nosotros          </a></li>
                <li><a href="{{ path('contacto')                                    }}">Contacto                </a></li>
                <li><a href="{{ path('usuario_registro')                            }}">Registro                </a></li>
                <li><a href="{{ path('pagina_estatica',{'pagina':'activar'})        }}">Activar                 </a></li><hr />
                <li><a href="{{ path('usuario_login')                               }}">Login                   </a></li>
                <li><a href="{{ path('usuario_logout')                              }}">Cerrar                  </a></li>
                <li><a href="{{ path('usuario_perfil')                              }}">Perfil                  </a></li>
                <li><a href="{{ path('usuario_perfilx')                             }}">perfilx                 </a></li>
            </ul>
            <ul style="float:left;width:10em;border:1px orange  solid;">
                <li><a href="{{ path('extranet_portada')                            }}">extranet_portada         </a></li>
            </ul>
        </div>
    </body>
</html>